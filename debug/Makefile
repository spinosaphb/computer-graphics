####### Compile

main.o: ../pacman/main.cpp ../pacman/gui_glut/gui.h \
		../pacman/gui_glut/extra.h \
		../pacman/bib/CameraDistante.h \
		../pacman/bib/Camera.h \
		../pacman/bib/Vetor3D.h \
		../pacman/bib/CameraJogo.h \
		../pacman/bib/Desenha.h \
		../pacman/bib/model3ds.h \
		../pacman/object/Sphere.h \
		../pacman/object/Object.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o main.o ../pacman/main.cpp

Camera.o: ../pacman/bib/Camera.cpp ../pacman/bib/Camera.h \
		../pacman/bib/Vetor3D.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o Camera.o ../pacman/bib/Camera.cpp

CameraDistante.o: ../pacman/bib/CameraDistante.cpp ../pacman/bib/CameraDistante.h \
		../pacman/bib/Camera.h \
		../pacman/bib/Vetor3D.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o CameraDistante.o ../pacman/bib/CameraDistante.cpp

